{
    "_version": "1.26.0",
    "sap.app": {
        "id": "MahindraWorkflow.workflowuimodule",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.8.4",
            "toolsId": "e4ef8a79-b49c-4dea-9b86-9e6c618df8be"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.82.2",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "MahindraWorkflow.workflowuimodule.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "MahindraWorkflow.workflowuimodule.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteMyTaskUI",
                    "pattern": ":?query:",
                    "target": [
                        "TargetMyTaskUI"
                    ]
                }
            ],
            "targets": {
                "TargetMyTaskUI": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "MyTaskUI",
                    "viewName": "MyTaskUI"
                }
            }
        },
        "rootView": {
            "viewName": "MahindraWorkflow.workflowuimodule.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "Mahindra-Workflow"
    },
    "sap.bpa.task": {
        "_version": "1.0.0",
        "outcomes": [
            {
                "id": "approve",
                "label": "Approve"
            },
            {
                "id": "reject",
                "label": "Reject"
            }
        ],
        "inputs": {
            "$schema": "http://json-schema.org/draft-07/schema",
            "title": "input",
            "type": "object",
            "required": [],
            "properties": {
                "poID": {
                    "type": "string",
                    "title": "PO ID",
                    "description": "PO ID"
                },
                "quotationID": {
                    "type": "string",
                    "title": "Quotation ID",
                    "description": "Quotation ID"
                },
                "deliveryLocation": {
                    "type": "string",
                    "title": "Delivery Location",
                    "description": "Delivery Location"
                },
                "pdfDocument": {
                    "type": "string",
                    "title": "PDF Document",
                    "description": "Link to the PDF document"
                },
                "companyName": {
                    "type": "string",
                    "title": "Company Name",
                    "description": "Name of the company"
                },
                "contactPerson": {
                    "type": "string",
                    "title": "Contact Person",
                    "description": "Name of the contact person"
                },
                "phoneNumber": {
                    "type": "string",
                    "title": "Phone Number",
                    "description": "Contact personâ€™s phone number"
                },
                "emailAddress": {
                    "type": "string",
                    "title": "Email Address",
                    "description": "Email address of the contact person"
                },
                "van": {
                    "type": "string",
                    "title": "Vendor Account Number",
                    "description": "Vendor Account Number"
                },
                "address": {
                    "type": "string",
                    "title": "Address",
                    "description": "Company or delivery address"
                },
                "documentType": {
                    "type": "string",
                    "title": "Document Type",
                    "description": "Type of the document"
                },
                "salesOrg": {
                    "type": "string",
                    "title": "Sales Organization",
                    "description": "Sales Organization"
                },
                "distributionChannel": {
                    "type": "string",
                    "title": "Distribution Channel",
                    "description": "Distribution Channel"
                },
                "division": {
                    "type": "string",
                    "title": "Division",
                    "description": "Division related to the sales order"
                },
                "vehicleCode": {
                    "type": "string",
                    "title": "Vehicle Code",
                    "description": "Code representing the vehicle"
                },
                "vehicleName": {
                    "type": "string",
                    "title": "Vehicle Name",
                    "description": "Name of the vehicle"
                },
                "vehicleColor": {
                    "type": "string",
                    "title": "Vehicle Color",
                    "description": "Color of the vehicle"
                },
                "quantity": {
                    "type": "number",
                    "title": "Quantity",
                    "description": "Number of vehicles"
                },
                "pricePerUnit": {
                    "type": "string",
                    "title": "Price Per Unit",
                    "description": "Price of each vehicle"
                },
                "actualPrice": {
                    "type": "string",
                    "title": "Actual Price",
                    "description": "Total price of vehicles before discount"
                },
                "discount": {
                    "type": "string",
                    "title": "Discount",
                    "description": "Discount on the vehicles"
                },
                "discountedPrice": {
                    "type": "string",
                    "title": "Discounted Price",
                    "description": "Total price of vehicles after discount"
                },
                "totalPrice": {
                    "type": "string",
                    "title": "Total Price",
                    "description": "Total price of the order"
                },
                "tax": {
                    "type": "string",
                    "title": "Tax",
                    "description": "Tax applied on the order"
                },
                "grandTotal": {
                    "type": "string",
                    "title": "Grand Total",
                    "description": "Total amount including tax"
                },
                "createdBy": {
                    "type": "string",
                    "title": "Created By",
                    "description": "The user who created the comment"
                },
                "createdAt": {
                    "type": "string",
                    "title": "Created At",
                    "description": "The date and time when the comment was created"
                },
                "commentText": {
                    "type": "string",
                    "title": "Comment Text",
                    "description": "The content of the comment"
                }
            }
        },
        "outputs": {
            "$schema": "http://json-schema.org/draft-07/schema",
            "title": "output",
            "type": "object",
            "required": [
                "comment"
            ],
            "properties": {
                "comment": {
                    "type": "string",
                    "title": "Comment",
                    "description": "Comment to buyer"
                }
            }
        },
        "category": "approval"
    }
}